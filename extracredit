<script>
    function submitQuiz() {
        var score = 0;
        var totalQuestions = 3;

        // Get the selected answers for each question
        var q1 = document.querySelector('input[name="q1"]:checked');
        var q2 = document.querySelector('input[name="q2"]:checked');
        var q3 = document.querySelector('input[name="q3"]:checked');

        // Check answers
        if (q1 && q1.value === "b") score++;  // Correct answer: B
        if (q2 && q2.value === "c") score++;  // Correct answer: C
        if (q3 && q3.value === "a") score++;  // Correct answer: A

        // Show the result
        document.getElementById("quizForm").style.display = "none";
        document.getElementById("result").style.display = "block";
        document.getElementById("score").innerHTML = score + " out of " + totalQuestions;
    }
</script>

